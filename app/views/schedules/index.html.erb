<p id="notice"><%= notice %></p>

<h1>Schedules</h1>

<table>
  <thead>
    <tr>
      <th>Campaign</th>
      <th>Flow name</th>
      <th>Flow order</th>
      <th>Starts on</th>
      <th>Ends on</th>
      <th>Workdays</th>
      <th>Status</th>
      <th>Post</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schedules.each do |schedule| %>
      <tr>
        <td><%= schedule.campaign_id %></td>
        <td><%= schedule.flow_name %></td>
        <td><%= schedule.flow_order %></td>
        <td><%= schedule.starts_on %></td>
        <td><%= schedule.ends_on %></td>
        <td><%= schedule.workdays %></td>
        <td><%= schedule.status %></td>
        <td><%= schedule.post_id %></td>
        <td><%= link_to 'Show', schedule %></td>
        <td><%= link_to 'Edit', edit_schedule_path(schedule) %></td>
        <td><%= link_to 'Destroy', schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Schedule', new_schedule_path %>
